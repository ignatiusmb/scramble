{
  "name": "@ignatiusmb/scramble",
  "author": "Ignatius Bagus",
  "description": "Scramble text and emulate decoding sequence",
  "version": "4.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "git add * && git clean -dfx -e node_modules",
    "check": "pnpm check:code && pnpm check:style",
    "check:code": "tsc --noEmit --target ES2020",
    "check:style": "prettier --check .",
    "format": "prettier --write ."
  },
  "types": "./index.d.ts",
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src/",
    "index.d.ts"
  ],
  "homepage": "https://scramble.js.org",
  "packageManager": "pnpm@8.6.5",
  "prettier": "mauss/prettier.json",
  "repository": "github:ignatiusmb/scramble",
  "bugs": "https://github.com/ignatiusmb/scramble/issues",
  "keywords": [
    "scramble",
    "random"
  ],
  "devDependencies": {
    "mauss": "^0.5.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6"
  }
}
